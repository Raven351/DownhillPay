<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="DownhillPayClient.Properties.APIEndpoints" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
            <section name="DownhillPayClient.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="DownhillPayClient.Properties.Settings.API_URI" connectionString="http://localhost:3000" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
    <applicationSettings>
        <DownhillPayClient.Properties.APIEndpoints>
            <setting name="Client" serializeAs="String">
                <value>client</value>
            </setting>
            <setting name="ClientID" serializeAs="String">
                <value>client?id=eq.</value>
            </setting>
        </DownhillPayClient.Properties.APIEndpoints>
        <DownhillPayClient.Properties.Settings>
            <setting name="RFID_USB_PORT" serializeAs="String">
                <value>COM3</value>
            </setting>
            <setting name="POS_ID" serializeAs="String">
                <value>0</value>
            </setting>
        </DownhillPayClient.Properties.Settings>
    </applicationSettings>
</configuration>
